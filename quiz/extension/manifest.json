{
    "name": "PubMed Quizzes",
    "version": "1.0.0",
    "description": "A quiz to check for understanding after reading a PubMed paper",
    "manifest_version": 3,
    "author": "Nooha Kawsar",
    "content_scripts": [
        {
          "matches": ["https://www.ncbi.nlm.nih.gov/pmc/articles/*"],
          "js": ["extension.js"],
          "css": ["sheet.css"]
        }
    ],

    "host_permissions": [
        "<all_urls>"
    ]

}


